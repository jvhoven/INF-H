{
  "name": "socket.io",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"node server/index\" \"yarn run build:dev\"",
    "build:dev": "webpack --watch --config config/webpack.dev.js",
    "build:prod": "webpack --config config/webpack.dev.js -p",
    "start:prod": "concurrently \"node server/index\" \"yarn run build:prod\""
  },
  "devDependencies": {
    "concurrently": "^3.3.0",
    "html-loader": "^0.4.4",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0"
  },
  "dependencies": {
    "angular": "^1.6.2",
    "angular-route": "^1.6.2",
    "angular-ui-router": "1.0",
    "express": "^4.14.1",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3"
  }
}

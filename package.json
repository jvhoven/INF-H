  "scripts": {
    "start": "concurrently \"node server/index\" \"yarn run build:dev\"",
    "build:dev": "webpack --watch --config webpack.config.js",
    "build:prod": "webpack --config webpack.config.js -p",
    "start:prod": "concurrently \"node server/index\" \"yarn run build:prod\"",
    "test": "jest --notify",
    "test:watch": "jest --watch --notify",
    "test:e2e": "scripts/e2e.sh"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.4",
    "babel": "^6.23.0",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "concurrently": "^3.3.0",
    "eslint": "^3.16.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-angular": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-standard": "^2.0.1",
    "html-loader": "^0.4.4",
    "jest": "^19.0.2",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0"
  },
  "dependencies": {
    "angular": "1.6.4",
    "angular-route": "^1.6.2",
    "angular-ui-router": "1.0",
    "express": "^4.14.1",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3"
  }
